{
  "name": "reclaimboardgame",
  "id": "reclaimboardgame",
  "title": "Reclaim Board Game",
  "description": "The Reclaim Board Game system for FoundryVTT!",
  "version": "0.0.2",
  "compatibility": {
    "minimum": "10",
    "verified": "10.285"
  },
  "authors": [
    {
      "name": "Adam Politanski@HiQ",
      "url": "https://hiq.se/",
      "email": "adam.politanski@hiq.se"
    }
  ],
  "esmodules": [
    "module/reclaimboardgame.mjs"
  ],
  "styles": [
    "css/reclaimboardgame.css"
  ],
  "scripts": [],
  "packs": [
    {
      "label": "Reclaim Tokens (Actors)",
      "type": "Actor",
      "name": "reclaim-tokens-actors",
      "path": "packs/reclaim-tokens-actors.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    },
    {
      "label": "Reclaim Resouces Tokens",
      "type": "Actor",
      "name": "reclaim-resouces-tokens",
      "path": "packs/reclaim-resouces-tokens.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    },
    {
      "label": "Reclaim Boards (Scenes)",
      "type": "Scene",
      "name": "reclaim-boards-scenes",
      "path": "packs/reclaim-boards-scenes.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    },
    {
      "label": "Reclaim Dice",
      "type": "RollTable",
      "name": "reclaim-dice",
      "path": "packs/reclaim-dice.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    },
    {
      "label": "Reclaim Decks",
      "type": "Cards",
      "name": "reclaim-decks",
      "path": "packs/reclaim-decks.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {}
    },
    {
      "label": "Macro Pack",
      "type": "Macro",
      "name": "macro-pack",
      "path": "packs/macro-pack.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    },
    {
      "label": "Reclaim Card Hands",
      "type": "Cards",
      "name": "reclaim-card-hands",
      "path": "packs/reclaim-card-hands.db",
      "system": "reclaimboardgame",
      "private": false,
      "flags": {},
      "index": []
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "url": "",
  "manifest": "http://localhost:8000/system.json",
  "download": "http://localhost:8000/packaged.zip",
  "license": "LICENSE.txt",
  "relationships": {
    "requires": [
      {
        "id": "hand-mini-bar",
        "type": "module",
        "manifest": "https://gitlab.com/pengrath/hand-mini-bar/-/raw/main/module.json",
        "compatibility": {}
      },
      {
        "id": "force-client-settings",
        "type": "module",
        "manifest": "https://gitlab.com/kimitsu_desu/force-client-settings/-/raw/main/module.json",
        "compatibility": {}
      },
      {
        "id": "settings-markers",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/reonZ/settings-markers/master/module.json",
        "compatibility": {}
      },
      {
        "id": "minimal-ui",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-minimal-ui/releases/latest/download/module.json",
        "compatibility": {}
      },
      {
        "id": "sidebar-resizer",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-sidebar-resizer/releases/latest/download/module.json",
        "compatibility": {}
      },
      {
        "id": "window-controls",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-window-controls/releases/latest/download/module.json",
        "compatibility": {}
      }
    ]
  }
}