{
  "name": "reclaimboardgame",
  "id": "reclaimboardgame",
  "title": "Reclaim Board Game",
  "description": "The Reclaim Board Game system for FoundryVTT!",
  "version": "0.0.2",
  "compatibility": {
    "minimum": "10",
    "verified": "10.285"
  },
  "authors": [
    {
      "name": "Adam Politanski@HiQ",
      "url": "https://hiq.se/",
      "email": "adam.politanski@hiq.se"
    }
  ],
  "esmodules": [
    "module/reclaimboardgame.mjs"
  ],
  "styles": [
    "css/reclaimboardgame.css"
  ],
  "scripts": [],
  "packs": [
    {
      "name": "reclaim-scenes",
      "label": "Game Boards",
      "path": "packs/reclaim-boards-scenes.db",
      "type": "Scene"
    },
    {
      "name": "reclaim-cards",
      "label": "Cards",
      "path": "packs/reclaim-decks.db",
      "type": "Scene"
    },
    {
      "name": "reclaim-tables",
      "label": "Dice",
      "path": "packs/reclaim-dice.db",
      "type": "Scene"
    },
    {
      "name": "reclaim-actors",
      "label": "Actors",
      "path": "packs/reclaim-tokens-actors.db",
      "type": "Scene"
    },
    {
      "name": "reclaim-tokens",
      "label": "Tokens",
      "path": "packs/reclaim-resources-tokens.db",
      "type": "Scene"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "url": "",
  "manifest": "http://localhost:8000/system.json",
  "download": "http://localhost:8000/packaged.zip",
  "license": "LICENSE.txt",
  "relationships": {
    "requires": [
      {
        "id": "hand-mini-bar",
        "type": "module",
        "manifest": "https://gitlab.com/pengrath/hand-mini-bar/-/raw/main/module.json",
        "compatibility": {}
      },
      {
        "id": "minimal-ui",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-minimal-ui/releases/latest/download/module.json",
        "compatibility": {}
      },
      {
        "id": "sidebar-resizer",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-sidebar-resizer/releases/latest/download/module.json",
        "compatibility": {}
      },
      {
        "id": "window-controls",
        "type": "module",
        "manifest": "https://github.com/saif-ellafi/foundryvtt-window-controls/releases/latest/download/module.json",
        "compatibility": {}
      }
    ]
  }
}